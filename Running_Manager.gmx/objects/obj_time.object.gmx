                  global.dayofmonth++;
                  global.ampm = "PM";
}
//Friday - Saturday
if(global.dayofweek == 4 &amp;&amp; global.hour &gt;= 5){
                  global.hour = 9;
                  global.minutes = 0;
                  global.dayofweek++;
                  global.dayofmonth++;
                  global.ampm = "AM";
}
//Saturday - Monday
if(global.dayofweek == 5 &amp;&amp; global.hour &gt;= 11){
                  global.hour = 2;
                  global.minutes = 30;
                  global.dayofweek++;
                  global.dayofmonth++;
                  global.ampm = "PM";
}
//Month to Year
if(global.dayofmonth &gt;= 30){
                  global.month++;
                  global.dayofmonth = 1;
}
//New Year
if(global.month &gt; 12){
                global.year++;
                global.month = 1;
}


//DOW NUM TO TEXT
if(global.dayofweek == 0){
             global.dowtext = "Monday"
}
else if(global.dayofweek == 1){
             global.dowtext = "Tuesday"
}
else if(global.dayofweek == 2){
